package net.te.employee.management.entities;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;
import lombok.ToString;

@Data
@NoArgsConstructor
@AllArgsConstructor
@ToString
public class PagedRequestBody {
	
	private int pageNumber;
	private int limit;
	private String searchText;
	
	public PagedRequestBody(int pageNumber, int limit) {
		super();
		this.pageNumber = pageNumber;
		this.limit = limit;
	}

}
